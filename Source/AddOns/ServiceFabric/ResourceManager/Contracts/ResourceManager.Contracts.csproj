<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\..\..\..\Build\common.props" />
  <Import Project="..\..\..\..\..\Build\key.props" />
  <Target Name="ProtoGen" BeforeTargets="BeforeBuild" Inputs="ResourceManager.proto" Outputs="Codegen">  
    <Exec Command="generate_protos.bat" />  
  </Target>
  <PropertyGroup>
    <AssemblyName>ResourceManager.Contracts</AssemblyName>
    <Description>The P# runtime and base services for service fabric.</Description>
    <AssemblyTitle>ResourceManager.Contracts</AssemblyTitle>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <PackageTags>asynchronous;event-driven;state-machines;systematic-testing;dotnet;csharp</PackageTags>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>
  <PropertyGroup>
    <OutputPath>..\..\bin\</OutputPath>
    <RootNamespace>ResourceManager.Contracts</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.6.0" />
    <PackageReference Include="Google.Protobuf.Tools" Version="3.6.0" />
    <PackageReference Include="Grpc" Version="1.13.0" />
    <PackageReference Include="Grpc.Core" Version="1.13.0" />
    <PackageReference Include="Grpc.Tools" Version="1.13.0" />
  </ItemGroup>
</Project>